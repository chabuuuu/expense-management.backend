{
    "server": {
        "port":  3001,
        "host": "localhost",
        "media_root": "http://localhost:3001"
    },
    "request-limit": {
        "limit": "100kb"
    },
    "cors": {
        "origin": "*",
        "methods": ["GET", "POST", "PUT", "DELETE", "UPDATE"],
        "allowedHeaders": ["X-Requested-With", "X-HTTP-Method-Override", "Content-Type", "Accept", "Observe", "Authorization"]
    },
    "helmet": false,
    "enviroment": "default",
    "morganFormat": "dev",
    "API_VERSION": "/api/v1",
    "swagger": {
        "enable": true
    },
    "currency_exchange_server": "https://open.er-api.com/v6/latest"
}